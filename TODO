Functions:
    langutil.init(
        dictionary: object | Array<Keyword>,
        language: string,
        {
            auto?: boolean,
            onDone: Function
        }
    ): unknown // returns the value of the function, if any
    langutil.setLanguage(
        language: string,
        {
            auto?: boolean,
            onDone: Function
        }
    ): unknown // returns the value of the function, if any
    langutil.localize(keyword: string): unknown
    langutil.localizeWith({
        keyword: 'HELLO',
        paramArray: [],
        casing: '',
        transform: ()=>{}
    }): unknown
    langutil.lang: {
        auto(): boolean
        current(): string
        allDefined(): Array<string>
    }
    langutil.logs: {
        hide(): void
