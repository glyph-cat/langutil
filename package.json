{
  "name": "langutil",
  "description": "Localization for JavaScript made simple",
  "version": "3.1.3",
  "author": "chin98edwin",
  "bin": {
    "langutil": "lu-scripts/"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "/lib",
    "index.d.ts",
    "index.js",
    "/react-additions",
    "/native-additions",
    "/lu-scripts",
    "scripts/post-install.js"
  ],
  "scripts": {
    "deploy": "node scripts/deploy.js",
    "install": "node scripts/post-install.js",
    "lint": "./node_modules/.bin/eslint lib/langutil.js native-additions/index.js native-additions/native-additions.js react-additions/index.js react-additions/react-additions.js dev-additions/ scripts/ lu-scripts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chin98edwin/langutil.git"
  },
  "keywords": [
    "accessibility",
    "language",
    "localization",
    "translation"
  ],
  "readme": "README.md",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chin98edwin/langutil/issues"
  },
  "homepage": "https://chin98edwin.github.io/langutil/",
  "peerDependencies": {
    "hoist-non-react-statics": "*"
  },
  "devDependencies": {
    "chalk": "^2.4.2",
    "eslint": "^5.16.0",
    "jest": "^24.9.0"
  },
  "jest": {
    "verbose": false,
    "testPathIgnorePatterns": [
      "docs"
    ]
  }
}