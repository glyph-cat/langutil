{
  "name": "langutil",
  "version": "4.0.3",
  "description": "Localization utility for JavaScript",
  "main": "lib/cjs/index.js",
  "module": "lib/es/index.js",
  "react-native": "lib/native/index.js",
  "unpkg": "lib/umd/index.js",
  "files": [
    "lib",
    "react"
  ],
  "types": "lib/types/index.d.ts",
  "scripts": {
    "deps": "node ./scripts/update-project-dependencies",
    "clean": "rm -rf ./lib ./react/lib ./temp ./yarn-error.log && mkdir temp lib react/lib",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "debug": "scope=\"debug\" jest",
    "test": "jest",
    "rollup": "rollup -c ./config/rollup.config.js",
    "types": "tsc --declaration",
    "api:main": "api-extractor run -c ./config/api-extractor.main.json --local --verbose",
    "postapi:main": "node scripts/post-api-main",
    "api:react": "api-extractor run -c ./config/api-extractor.react.json --local --verbose",
    "postapi:react": "node scripts/post-api-react",
    "api": "yarn api:main && yarn api:react",
    "build": "yarn rollup && yarn types && yarn api",
    "postbuild": "rm -r ./temp/tsc",
    "all": "yarn clean && yarn lint:fix && yarn build && node scripts/mid-all && yarn test && yarn pack",
    "ci:test": "yarn lint && yarn build && yarn test"
  },
  "keywords": [
    "Accessbility",
    "Language",
    "Language Utility",
    "Localization",
    "Translation"
  ],
  "author": "glyph-cat",
  "readme": "README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/glyph-cat/langutil.git"
  },
  "bugs": {
    "url": "https://github.com/glyph-cat/langutil/issues"
  },
  "homepage": "https://github.com/glyph-cat/langutil#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@glyph-cat/eslint-config": "^1.0.0-alpha.0",
    "@glyph-cat/react-test-utils": "^0.1.2-experimental.0",
    "@jest/types": "^28.1.0",
    "@microsoft/api-extractor": "^7.24.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-native": "^0.67.7",
    "@types/react-test-renderer": "^18.0.0",
    "eslint": "^8.15.0",
    "eslint-plugin-functional": "^4.2.1",
    "hoist-non-react-statics": "^3.3.2",
    "jest": "^28.1.0",
    "jsdom": "^19.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-native": "^0.68.2",
    "react-relink": "^3.1.3",
    "react-test-renderer": "^18.1.0",
    "rollup": "^2.73.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-node": "^10.7.0",
    "tsc": "^2.0.4",
    "typescript": "^4.6.4"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "dependencies": {
    "use-sync-external-store": "^1.1.0"
  }
}