{
  "name": "langutil",
  "description": "Localization for JavaScript made simple. ",
  "version": "2.4.0",
  "author": "chin98edwin",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "/lib",
    "index.d.ts",
    "index.js",
    "/react-additions",
    "/native-additions"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/eslint lib/langutil.dev.js native-additions react-additions scripts/",
    "lint-fix": "./node_modules/.bin/eslint lib/langutil.dev.js native-additions react-additions scripts/ --fix",
    "test": "jest",
    "deploy-patch": "jest && node scripts/increaseVersionNumber --patch && npm publish",
    "deploy-minor": "jest && node scripts/increaseVersionNumber --minor && npm publish",
    "deploy-major": "jest && node scripts/increaseVersionNumber --major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chin98edwin/langutil.git"
  },
  "keywords": [
    "accessibility",
    "language",
    "localization",
    "translation"
  ],
  "readme": "README.md",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chin98edwin/langutil/issues"
  },
  "homepage": "https://github.com/chin98edwin/langutil#readme",
  "devDependencies": {
    "chalk": "^2.4.2",
    "eslint": "^5.16.0",
    "jest": "^24.5.0"
  },
  "jest": {
    "verbose": false,
    "testPathIgnorePatterns": [
      "/__tests__/baseImplementation/",
      "/demo"
    ]
  }
}
