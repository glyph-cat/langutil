{
  "name": "langutil",
  "version": "4.0.0-canary.0",
  "description": "Localization utility for JavaScript",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "react-native": "dist/native/index.js",
  "unpkg": "dist/umd/index.js",
  "files": [
    "dist",
    "react",
    "index.d.ts"
  ],
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist ./react/dist ./temp ./yarn-error.log && mkdir temp dist react/dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "debug": "scope=\"debug\" jest",
    "test": "jest",
    "rollup": "rollup -c ./config/rollup.config.js",
    "tsc": "tsc --declaration",
    "api:main": "api-extractor run -c ./config/api-extractor.main.json --local --verbose",
    "api:react": "api-extractor run -c ./config/api-extractor.react.json --local --verbose",
    "api": "yarn api:main && yarn api:react",
    "build": "yarn run rollup && yarn run tsc && yarn run api",
    "postbuild": "rm -r ./temp/tsc",
    "all": "yarn lint:fix && yarn build && yarn test && yarn pack"
  },
  "keywords": [
    "Accessbility",
    "Language",
    "Language Utility",
    "Localization",
    "Translation"
  ],
  "author": "chin98edwin",
  "readme": "README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chin98edwin/langutil.git"
  },
  "bugs": {
    "url": "https://github.com/chin98edwin/langutil/issues"
  },
  "homepage": "https://github.com/chin98edwin/langutil#readme",
  "dependencies": {
    "@types/hoist-non-react-statics": "^3.3.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/preset-flow": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@microsoft/api-extractor": "^7.18.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-functional": "^3.2.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "hoist-non-react-statics": "^3.3.2",
    "jest": "^27.0.6",
    "jsdom": "^16.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.64.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.53.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^10.1.0",
    "tsc": "^2.0.3",
    "typescript": "^4.3.5"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  }
}
